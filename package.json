{
  "name": "koa-compose",
  "description": "compose Koa middleware",
  "repository": "koajs/compose",
  "version": "3.1.0",
  "browser": "./browser.js",
  "keywords": [
    "koa",
    "middleware",
    "compose"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "any-promise": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "co": "^4.6.0",
    "istanbul": "^0.4.2",
    "matcha": "^0.6.1",
    "mocha": "^2.4.5",
    "should": "^2.0.0",
    "standard": "^6.0.8"
  },
  "scripts": {
    "bench": "matcha bench/bench.js",
